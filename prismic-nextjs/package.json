{
  "name": "prismic-nextjs",
  "version": "0.0.1",
  "description": "Sandbox to learn Prismic/NextJS",
  "private": true,
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=1 next build",
    "analyze:server": "ANALYZE_SERVER=1 next build",
    "build": "next build",
    "dev": "node server/server.js",
    "dev:inspect": "node --inspect server/server.js",
    "deploy:qa": "now",
    "docker:build": "docker build --build-arg NPM_TOKEN=${NPM_TOKEN} -t prismic-nextjs .",
    "docker:run": "docker run -e \"NPM_TOKEN=${NPM_TOKEN}\" --rm --name prismic-nextjs -p 3001:3001 prismic-nextjs",
    "docker:stop": "docker stop prismic-nextjs",
    "start": "NODE_ENV=production node server/server.js",
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "secrets:edit": "sops .enc.env",
    "secrets:decrypt": "sops -d .enc.env > .env",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TangoGroup/prismic-nextjs.git"
  },
  "author": "The Gloo Web Team",
  "license": "UNLICENSED",
  "homepage": "https://github.com/TangoGroup/prismic-nextjs#readme",
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@gloojs/bass-ui": "1.0.9",
    "@gloojs/next-express": "1.2.0",
    "@gloojs/material-ui-components": "1.0.0",
    "@gloojs/material-ui-theme": "1.0.0",
    "@gloojs/next-lib": "1.5.0",
    "@gloojs/next-nprogress": "1.2.0",
    "@gloojs/ui-components": "1.2.0",
    "@material-ui/core": "4.5.2",
    "@material-ui/icons": "4.2.0",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "compression": "1.7.3",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-robots-txt": "0.4.0",
    "helmet": "3.20.0",
    "hoist-non-react-statics": "3.0.1",
    "lodash": "4.17.11",
    "next": "9.4.2",
    "next-images": "1.0.1",
    "nocache": "2.1.0",
    "prop-types": "15.6.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/traverse": "7.1.0",
    "@babel/types": "7.1.2",
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@gloojs/eslint-config": "1.0.6",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "8.0.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "1.12.2",
    "eslint": "5.7.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-disable": "1.0.3",
    "eslint-plugin-emotion": "10.0.7",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.25.1",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest": "23.6.0",
    "webpack-bundle-analyzer": "2.13.1"
  }
}
